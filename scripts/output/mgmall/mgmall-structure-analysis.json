{
  "name": "문경 새제의아침",
  "url": "https://mgmall.cyso.co.kr/",
  "region": "경북",
  "platform": "youngcart5",
  "categories": {
    "structure": "/shop/list.php?ca_id={categoryId}",
    "mainCategories": [
      {
        "name": "쌀/잡곡",
        "id": "mg10",
        "url": "/shop/list.php?ca_id=mg10",
        "subcategories": [
          { "name": "쌀", "id": "mg1010" },
          { "name": "기타잡곡", "id": "mg1020" }
        ]
      },
      {
        "name": "과일/채소",
        "id": "mg20",
        "url": "/shop/list.php?ca_id=mg20"
      },
      {
        "name": "가공식품",
        "id": "mg30",
        "url": "/shop/list.php?ca_id=mg30"
      }
    ],
    "urlPattern": "/shop/list.php?ca_id={categoryId}"
  },
  "products": {
    "htmlStructure": "youngcart5-standard",
    "containerSelector": ".sct_li",
    "dataLocation": "inside .sct wrapper",
    "selectors": {
      "wrapper": ".sct",
      "item": ".sct_li",
      "image": ".sct_img img",
      "title": ".sct_txt a",
      "price": ".sct_cost",
      "link": ".sct_txt a",
      "icon": ".sct_icon"
    }
  },
  "pagination": {
    "type": "standard",
    "selector": ".pg_wrap",
    "urlPattern": "&page={pageNumber}",
    "hasNext": true,
    "hasPrev": false
  },
  "dynamicLoading": {
    "required": false,
    "type": "static"
  },
  "notes": {
    "특이사항": "영카트5 기반 쇼핑몰 (cyso.co.kr 도메인)",
    "제품URL패턴": "/shop/item.php?it_id={productId}",
    "카테고리코드": "mg로 시작하는 계층구조",
    "공통플랫폼": "안동장터와 동일한 cyso.co.kr 플랫폼 사용"
  }
}